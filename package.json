{
  "name": "ovpnrental_agent",
  "version": "1.0.1",
  "description": "VPN server-side tool to leverage basic certificate operations",
  "main": "index.ts",
  "scripts": {
    "dev": "nodemon ./src/index.ts",
    "build": "tsc -p tsconfig.json",
    "pkg": "rm -f dist/* && pkg tsbuild/src/index.js -t node18-linux-x64 --out-path dist && mv dist/index dist/ovpnrental_agent"
  },
  "pkg": {
    "scripts": "tsbuild/**/*.js",
    "assets": "views/**/*",
    "outputPath": "dist"
  },
  "author": "rinfly97",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.4",
    "pkg": "^5.8.1",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.496.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "winston-loki": "^6.0.8"
  }
}
